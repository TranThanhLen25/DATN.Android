<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragment_phieudoiphong">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/chitietphong"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/scr_chitietphong">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/headderchitietphong"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintEnd_toEndOf="@id/chitietphong"
            app:layout_constraintStart_toStartOf="@id/chitietphong"
            app:layout_constraintTop_toTopOf="@+id/chitietphong">


            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/chitietheaderphieunhanphong"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintStart_toStartOf="@id/headderchitietphong"
                app:layout_constraintTop_toTopOf="@+id/headderchitietphong">

                <TextView
                    android:id="@+id/tv_chitietphong"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/bg_tenphieu"
                    android:padding="10dp"
                    android:text="@string/tv_phieudoiphong"
                    android:textSize="@dimen/heading_2"
                    android:textStyle="bold"
                    app:layout_constraintStart_toStartOf="@id/chitietheaderphieunhanphong"
                    app:layout_constraintTop_toTopOf="@id/chitietheaderphieunhanphong" />


                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/tv_sochungtu_chitietphong"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    app:layout_constraintStart_toStartOf="@id/chitietheaderphieunhanphong"
                    app:layout_constraintTop_toBottomOf="@id/tv_chitietphong" />

            </androidx.constraintlayout.widget.ConstraintLayout>

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/mainphieunhanphong"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintEnd_toEndOf="@id/chitietphong"
            app:layout_constraintStart_toStartOf="@id/chitietphong"
            app:layout_constraintTop_toBottomOf="@id/headderchitietphong">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/ctl_doiphong_vitribandau"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">


                <ImageButton
                    android:id="@+id/imgbtn_doiphong_chonphong1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@color/white"
                    android:padding="15dp"
                    android:src="@drawable/ic_additem"
                    android:visibility="invisible"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/ctl_doiphong_vitribandau"
                    tools:ignore="SpeakableTextPresentCheck" />

                <TextView
                    android:id="@+id/tv_doiphong_vitribandau"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/tv_vitribandau"
                    android:textSize="20dp"
                    app:layout_constraintBottom_toBottomOf="@+id/ctl_doiphong_vitribandau"
                    app:layout_constraintStart_toEndOf="@+id/imgbtn_doiphong_chonphong1"
                    app:layout_constraintTop_toTopOf="@+id/ctl_doiphong_vitribandau" />

                <TextView
                    android:id="@+id/tv_doiphong_data_vitribandau"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="101"
                    android:textSize="20sp"
                    app:layout_constraintBottom_toBottomOf="@+id/ctl_doiphong_vitribandau"
                    app:layout_constraintStart_toEndOf="@+id/tv_doiphong_vitribandau"
                    app:layout_constraintTop_toTopOf="@+id/ctl_doiphong_vitribandau" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/ctl_doiphong_vitridoi"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/ctl_doiphong_vitribandau">


                <ImageButton
                    android:id="@+id/imgbtn_doiphong_chonphong"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@color/white"
                    android:padding="15dp"
                    android:src="@drawable/ic_additem"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/ctl_doiphong_vitridoi"
                    tools:ignore="SpeakableTextPresentCheck" />

                <TextView
                    android:id="@+id/tv_doiphong_vitridoi"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/tv_vitridoi"
                    android:textSize="20dp"
                    app:layout_constraintBottom_toBottomOf="@+id/ctl_doiphong_vitridoi"
                    app:layout_constraintStart_toEndOf="@+id/imgbtn_doiphong_chonphong"
                    app:layout_constraintTop_toTopOf="@+id/ctl_doiphong_vitridoi" />

                <TextView
                    android:id="@+id/tv_doiphong_data_vitridoi"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="101"
                    android:textSize="20sp"
                    app:layout_constraintBottom_toBottomOf="@+id/ctl_doiphong_vitridoi"
                    app:layout_constraintStart_toEndOf="@+id/tv_doiphong_vitridoi"
                    app:layout_constraintTop_toTopOf="@+id/ctl_doiphong_vitridoi" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <TextView
                android:id="@+id/tv_hoten"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="8dp"
                android:text="@string/tv_hoten"
                android:textColor="@color/black"
                android:textSize="@dimen/textview_tk_mk"
                app:layout_constraintStart_toStartOf="@+id/mainphieunhanphong"
                app:layout_constraintTop_toBottomOf="@+id/ctl_doiphong_vitridoi"
                tools:ignore="SpUsage" />

            <EditText
                android:id="@+id/et_hoten_phieunhanphong"
                android:layout_width="match_parent"
                android:layout_height="48dp"
                android:background="@drawable/bg_edittext"
                android:importantForAutofill="no"
                android:inputType="textPersonName"
                app:layout_constraintTop_toBottomOf="@+id/tv_hoten"
                tools:ignore="SpeakableTextPresentCheck" />

            <TextView
                android:id="@+id/tv_cccd_phieunhanphong"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="8dp"
                android:text="@string/tv_cccd"
                android:textColor="@color/black"
                android:textSize="@dimen/textview_tk_mk"
                app:layout_constraintStart_toStartOf="@+id/mainphieunhanphong"
                app:layout_constraintTop_toBottomOf="@+id/et_hoten_phieunhanphong" />

            <EditText
                android:id="@+id/et_cccd_phieunhanphong"
                android:layout_width="match_parent"
                android:layout_height="48dp"
                android:background="@drawable/bg_edittext"
                android:importantForAutofill="no"
                android:inputType="number"
                app:layout_constraintTop_toBottomOf="@+id/tv_cccd_phieunhanphong"
                tools:ignore="SpeakableTextPresentCheck" />


        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/footer_phieunhanphong"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:padding="14dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent">

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/action_doiphong"
                app:layout_constraintBottom_toBottomOf="@+id/footer_phieunhanphong"
                app:layout_constraintEnd_toEndOf="@+id/footer_phieunhanphong"
                app:layout_constraintTop_toTopOf="@+id/footer_phieunhanphong" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>


</FrameLayout>